# Default LLM provider preference: openai, gemini, claude, groq, or ollama
LLM_PROVIDER=openai

# Local model when using Ollama
OLLAMA_MODEL=qwen2.5-coder:14b

# OpenAI
OPENAI_API_KEY=your-openai-key
OPENAI_MODEL=gpt-4o-mini

# Anthropic Claude
ANTHROPIC_API_KEY=your-anthropic-key
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# Groq
GROQ_API_KEY=your-groq-key
groq_model=llama-3.3-70b-versatile

# Google Gemini
GOOGLE_API_KEY=your-google-key
GEMINI_MODEL=gemini-2.5-flash
GEMINI_VISION_MODEL=gemini-2.5-flash

# Unsplash (stock photo fallback)
UNSPLASH_ACCESS_KEY=your-unsplash-access
UNSPLASH_SECRET=your-unsplash-secret
UNSPLASH_CLIENT_ID=your-unsplash-client-id

# Hugging Face (image generation)
HF_TOKEN=your-hf-token
HF_MODEL=black-forest-labs/FLUX.1-dev
# HF_MODEL=stabilityai/stable-diffusion-3-5-large

# Optional Playwright path for screenshots
PLAYWRIGHT_BROWSERS_PATH=.cache/ms-playwright
